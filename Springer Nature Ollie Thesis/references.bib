WOS:000367436400038},
	keywords = {adaptations, body, gymnastics, human postural control, integration, motor learning, postural control, randomized controlled-trial, sensory integration, soleus h-reflex, stability, support, sway, upright stance},
	pages = {2967--2982},
}

@article{palmer_enhanced_2016,
