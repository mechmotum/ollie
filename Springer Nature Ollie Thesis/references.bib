	keywords = {Distance-deceleration-time profile, Near crash, Roadside   LiDAR, Skateboarder, behavior, measured traffic conflicts, risk, safety performance, signalized intersections, surrogate measure, system, tracking, vehicle detection, vissim simulation-model},
