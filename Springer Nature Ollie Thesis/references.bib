@article{liu_skateboard_2018,
	title = {Skateboard deck materials selection},
	volume = {128},
	doi = {10.1088/1755-1315/128/1/012170},
	abstract = {The goal of this project was to identify the ideal material for a skateboard deck under \$200 in price, minimizing the weight. The material must have a fracture toughness of 5 MPa/m2, have a minimum lifetime of 10, 000 cycles and must not experience brittle fracture. Both single material and hybrid solutions were explored. When further selecting to minimize weight, woods were found to be the best material. Titanium alloy-wood composites were explored to determine the optimal percentage composition of each material.A sandwich panel hybrid of 50\% titanium alloy and 50\% wood (Ti-Wood) was found to be the optimum material, performing better than the currently used plywood.},
