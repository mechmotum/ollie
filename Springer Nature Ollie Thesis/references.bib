Publisher: Amer Physiological Soc
WOS:000355000900063},
	keywords = {adaptation, arm, consolidation, control   strategies, error, feedback, haptic guidance, internal-model, intrinsic motivation, motivation, motor learning, motor skill, movement variability, robotic   training, self-efficacy},
	pages = {2682--2691},
}

@article{van_dieen_learning_2015,
	title = {Learning to balance on one leg: motor strategy and sensory weighting},
	volume = {114},
	issn = {0022-3077},
	shorttitle = {Learning to balance on one leg},
