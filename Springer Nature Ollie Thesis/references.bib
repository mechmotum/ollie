Publisher: Science Press
WOS:000320739100020},
	keywords = {gaits, locomotion, mobile robot, motion, nonholonomic system, path planning, point-to-point maneuver, rolling motion, skateboard, snakeboard, systems},
	pages = {1499--1516},
}

