	note = {Place: Abingdon
Publisher: Routledge Journals, Taylor \& Francis Ltd
WOS:000398101600012},
	keywords = {Anticipatory behavior, eye tracking, information, locomotion, obstacle, ollie, perception-action   coupling, perception–action coupling, skateboarding, step},
	pages = {101--107},
}

