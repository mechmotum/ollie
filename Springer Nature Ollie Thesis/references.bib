Publisher: Routledge Journals, Taylor \& Francis Ltd
WOS:000430501000002},
	keywords = {Gymnastics, ankle, balance, children, feedback, inverted stance, memory, postural execution},
	pages = {17--31},
}

@article{mcnitt-gray_using_2015,
	title = {Using technology and engineering to facilitate skill acquisition and improvements in performance},
